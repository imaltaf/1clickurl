{
  "name": "Bulk URL Opener",
  "short_name": "Bulk URL Opener",
  "author": "Euan Riggans",
  "manifest_version": 3,
  "version": "2.0.2",
  "description": "Browser addon to open multiple urls with one click. With many more tools to help you manage urls.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_icon": "icon/window.png",
    "default_title": "Bulk URL Open",
    "default_popup": "popup.html"
    
  },
  "options_ui": {
    "page": "/pages/settings/index.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icon/window.png",
    "32": "icon/window.png",
    "64": "icon/window.png",
    "128": "icon/window.png"
  }
}
